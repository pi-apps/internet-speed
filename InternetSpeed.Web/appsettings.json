{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SourceConnectionString": "Your-connetion-string"
  },
  "PiNetwork": {
    "ApiKey": "Your-api-key"
  }
  ,

  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      },
      "Https": {
        "Url": "https://+:443",
        "Certificate": {
          "Path": "/https/example.crt",
          "KeyPath": "/https/example.key"
        }
      }
    }
  }
}